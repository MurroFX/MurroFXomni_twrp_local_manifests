    <project path="bootable/recovery" name="android_bootable_recovery" remote="omnirom" revision="android-9.0" />

<!-- Use OmniROM busybox -->
    <project path="external/busybox" name="android_external_busybox" remote="TeamWin" revision="android-9.0" />
</manifest>
